{
    "private": true,
    "name": "@enteocode/artic",
    "version": "2.1.2",
    "description": "Personal list of favorite art pieces of Art Institute of Chicago (ARTIC)",
    "type": "module",
    "scripts": {
        "start": "docker-compose -f docker-compose.yaml -f docker-compose.development.yaml up",
        "build": "rollup -c --environment NODE_ENV:production",
        "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
        "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "prepare": "husky",
        "release": "standard-version"
    },
    "author": {
        "name": "Ádám Székely",
        "url": "https://www.linkedin.com/in/enteocode/"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.2",
        "@fastify/helmet": "^13.0.1",
        "@keyv/redis": "^4.3.2",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^11.0.12",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.12",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/platform-fastify": "^11.0.12",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/typeorm": "^11.0.0",
        "bcrypt": "^5.1.1",
        "cache-manager": "^6.4.1",
        "cacheable": "^1.8.9",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "email-addresses": "^5.0.0",
        "helmet": "^8.1.0",
        "ioredis": "^5.6.0",
        "keyv": "^5.3.2",
        "mysql2": "^3.14.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typeorm": "^0.3.21",
        "uuid": "^11.1.0",
        "xxhash-wasm": "^1.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@fastify/static": "^8.1.1",
        "@nestjs/cli": "^11.0.5",
        "@nestjs/swagger": "^11.1.0",
        "@nestjs/testing": "^11.0.12",
        "@rollup/plugin-run": "^3.1.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/bcrypt": "^5.0.2",
        "@types/node": "^20.17.28",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "read-pkg": "^9.0.1",
        "rollup": "^2.79.2",
        "standard-version": "^9.5.0",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.0",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "testEnvironment": "node",
        "rootDir": "src",
        "testRegex": ".*\\.test\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage"
    }
}
