{
  "private": true,
  "name": "@icf/backend-trial",
  "version": "1.0.0",
  "description": "Personal list of favorite art pieces of Art Institute of Chicago (ARTIC)",
  "type": "module",
  "scripts": {
    "start": "docker-compose -f docker-compose.yaml -f docker-compose.development.yaml up",
    "build": "rollup -c --environment NODE_ENV:production",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "standard-version"
  },
  "author": {
    "name": "Ádám Székely",
    "url": "https://www.linkedin.com/in/enteocode/",
    "email": "stereotribe@hotmail.com"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/schematics": "^8.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/testing": "^8.4.3",
    "@nestjs/typeorm": "~8.0.2",
    "@rollup/plugin-run": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "helmet": "^5.0.1",
    "ioredis": "^4.28.5",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.76.0",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "~0.2.45",
    "uuid": "^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testEnvironment": "node",
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
